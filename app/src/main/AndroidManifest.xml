<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="su.nosite.smth.waih">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="quiz"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="su.nosite.smth.quizapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="su.nosite.smth.quizapp.AddingQuestionActivity" />
        <activity android:name="su.nosite.smth.quizapp.StandartModeActivity" />
        <activity android:name="su.nosite.smth.quizapp.CustomModeActivity" />
        <activity
            android:name="su.nosite.smth.quizapp.RatingActivity"
            android:label="@string/title_activity_rating"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="su.nosite.smth.quizapp.QuizStandartActivity" />
        <activity android:name="su.nosite.smth.quizapp.QuizCustomActivity"></activity>
    </application>

</manifest>